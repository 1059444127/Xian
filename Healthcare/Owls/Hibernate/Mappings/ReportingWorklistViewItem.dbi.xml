<?xml version="1.0" encoding="utf-8" ?>
<!--  

This file adds a number of supplementary indexes to the Healthcare model.
Note that all entity and enumeration references are already indexed automatically,
so the only fields that are explicitly marked for indexing here are generally 
time-stamps and other value fields that are known to be used in queries, and only
on key tables that play a central role in the workflow queries.

-->
<indexes>
  <!-- Patient Profile-->
  <index table="owls_ReportingWorklistViewItem_" columns="PatientProfileMrnId_"/>
  <index table="owls_ReportingWorklistViewItem_" columns="PatientProfileHealthcardId_"/>
  <index table="owls_ReportingWorklistViewItem_" columns="PatientProfileFamilyName_, PatientProfileGivenName_"/>
  
  <!-- Order -->
  <index table="owls_ReportingWorklistViewItem_" columns="OrderAccessionNumber_"/>

  <!-- Procedure -->

  <!-- Procedure Step-->
  <index table="owls_ReportingWorklistViewItem_" columns="ProcedureStepClassName_"/>

  <!-- Report Part -->
</indexes>
